<div class="space-y-6">
  <!-- Summary Section -->
  <div class="bg-white border border-gray-200 rounded-lg p-6">
    <h3 class="text-lg font-medium text-gray-900 mb-4">Analysis Summary</h3>
    
    <div class="grid grid-cols-1 md:grid-cols-3 gap-4">
      <!-- Execution Time -->
      <div class="bg-gray-50 rounded-lg p-4">
        <div class="flex items-center">
          <svg class="h-5 w-5 text-gray-400 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
          </svg>
          <span class="text-sm text-gray-500">Execution Time</span>
        </div>
        <p class="mt-2 text-2xl font-semibold text-gray-900">
          <%= number_with_precision(analysis_request.total_execution_time || 0, precision: 2) %>s
        </p>
      </div>
      
      <!-- Steps Executed -->
      <div class="bg-gray-50 rounded-lg p-4">
        <div class="flex items-center">
          <svg class="h-5 w-5 text-gray-400 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2"></path>
          </svg>
          <span class="text-sm text-gray-500">Steps Executed</span>
        </div>
        <p class="mt-2 text-2xl font-semibold text-gray-900">
          <%= analysis_request.execution_steps.completed.count %>
        </p>
      </div>
      
      <!-- Data Processed -->
      <div class="bg-gray-50 rounded-lg p-4">
        <div class="flex items-center">
          <svg class="h-5 w-5 text-gray-400 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 7v10c0 2.21 3.582 4 8 4s8-1.79 8-4V7M4 7c0 2.21 3.582 4 8 4s8-1.79 8-4M4 7c0-2.21 3.582-4 8-4s8 1.79 8 4"></path>
          </svg>
          <span class="text-sm text-gray-500">Data Source</span>
        </div>
        <p class="mt-2 text-lg font-semibold text-gray-900">
          <%= analysis_request.dataset.name %>
        </p>
      </div>
    </div>
  </div>
  
  <!-- Key Findings -->
  <% if analysis_request.metadata['interpretation'].present? %>
    <div class="bg-white border border-gray-200 rounded-lg p-6">
      <h3 class="text-lg font-medium text-gray-900 mb-4">Key Findings</h3>
      
      <div class="prose max-w-none">
        <%= simple_format(analysis_request.metadata['interpretation']) %>
      </div>
    </div>
  <% end %>
  
  <!-- Visualizations -->
  <% visualizations = analysis_request.execution_steps.flat_map { |s| s.metadata['generated_files'] || [] }.select { |f| f['name']&.match?(/\.(png|jpg|svg)$/i) } %>
  <% if visualizations.any? %>
    <div class="bg-white border border-gray-200 rounded-lg p-6">
      <h3 class="text-lg font-medium text-gray-900 mb-4">Visualizations</h3>
      
      <div class="grid grid-cols-1 md:grid-cols-2 gap-4">
        <% visualizations.each do |viz| %>
          <div class="border rounded-lg overflow-hidden">
            <img src="<%= viz['path'] %>" alt="<%= viz['name'] %>" class="w-full h-auto">
            <div class="px-3 py-2 bg-gray-50 text-sm text-gray-600">
              <%= viz['name'] %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
  
  <!-- Data Tables -->
  <% data_outputs = analysis_request.execution_steps.select { |s| s.output.present? && s.step_type == 'statistical_analysis' } %>
  <% if data_outputs.any? %>
    <div class="bg-white border border-gray-200 rounded-lg p-6">
      <h3 class="text-lg font-medium text-gray-900 mb-4">Statistical Results</h3>
      
      <% data_outputs.each do |step| %>
        <div class="mb-4">
          <h4 class="text-sm font-medium text-gray-700 mb-2"><%= step.description %></h4>
          <div class="bg-gray-50 rounded-lg p-3 overflow-x-auto">
            <pre class="text-xs font-mono"><%= step.output %></pre>
          </div>
        </div>
      <% end %>
    </div>
  <% end %>
  
  <!-- Export Options -->
  <div class="bg-white border border-gray-200 rounded-lg p-6">
    <h3 class="text-lg font-medium text-gray-900 mb-4">Export Results</h3>
    
    <div class="flex space-x-4">
      <%= link_to export_analysis_request_path(analysis_request, format: :pdf),
          class: "inline-flex items-center px-4 py-2 border border-gray-300 rounded-md shadow-sm text-sm font-medium text-gray-700 bg-white hover:bg-gray-50",
          data: { turbo: false } do %>
        <svg class="mr-2 h-5 w-5 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 10v6m0 0l-3-3m3 3l3-3m2 8H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
        </svg>
        Download PDF Report
      <% end %>
      
      <%= link_to export_analysis_request_path(analysis_request, format: :csv),
          class: "inline-flex items-center px-4 py-2 border border-gray-300 rounded-md shadow-sm text-sm font-medium text-gray-700 bg-white hover:bg-gray-50",
          data: { turbo: false } do %>
        <svg class="mr-2 h-5 w-5 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 10v6m0 0l-3-3m3 3l3-3m2 8H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
        </svg>
        Download CSV Data
      <% end %>
      
      <%= link_to share_analysis_request_path(analysis_request),
          class: "inline-flex items-center px-4 py-2 border border-gray-300 rounded-md shadow-sm text-sm font-medium text-gray-700 bg-white hover:bg-gray-50",
          data: { 
            turbo_frame: "share_modal",
            turbo_action: "replace"
          } do %>
        <svg class="mr-2 h-5 w-5 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8.684 13.342C8.886 12.938 9 12.482 9 12c0-.482-.114-.938-.316-1.342m0 2.684a3 3 0 110-2.684m9.032 4.026a9.001 9.001 0 01-7.432 0m9.032-4.026A9.001 9.001 0 0112 3c-4.474 0-8.268 3.12-9.032 7.326m0 0A9 9 0 0012 21c4.474 0 8.268-3.12 9.032-7.326"></path>
        </svg>
        Share Results
      <% end %>
    </div>
  </div>
  
  <!-- Technical Details (Collapsible) -->
  <details class="bg-white border border-gray-200 rounded-lg p-6">
    <summary class="cursor-pointer text-lg font-medium text-gray-900 select-none">
      Technical Details
    </summary>
    
    <div class="mt-4 space-y-4">
      <!-- Models Used -->
      <div>
        <h4 class="text-sm font-medium text-gray-700 mb-2">AI Models Used</h4>
        <div class="bg-gray-50 rounded-lg p-3">
          <% (analysis_request.metadata['selected_models'] || {}).each do |task, model| %>
            <div class="flex justify-between py-1 text-sm">
              <span class="text-gray-600"><%= task.humanize %>:</span>
              <span class="font-mono text-gray-900"><%= model %></span>
            </div>
          <% end %>
        </div>
      </div>
      
      <!-- Token Usage -->
      <% if analysis_request.metadata['total_tokens_used'].present? %>
        <div>
          <h4 class="text-sm font-medium text-gray-700 mb-2">Token Usage</h4>
          <div class="bg-gray-50 rounded-lg p-3">
            <div class="flex justify-between py-1 text-sm">
              <span class="text-gray-600">Input Tokens:</span>
              <span class="font-mono text-gray-900"><%= number_with_delimiter(analysis_request.metadata['total_tokens_used']['input']) %></span>
            </div>
            <div class="flex justify-between py-1 text-sm">
              <span class="text-gray-600">Output Tokens:</span>
              <span class="font-mono text-gray-900"><%= number_with_delimiter(analysis_request.metadata['total_tokens_used']['output']) %></span>
            </div>
            <div class="flex justify-between py-1 text-sm border-t pt-1">
              <span class="text-gray-600 font-medium">Total:</span>
              <span class="font-mono text-gray-900 font-medium">
                <%= number_with_delimiter(
                      (analysis_request.metadata['total_tokens_used']['input'] || 0) + 
                      (analysis_request.metadata['total_tokens_used']['output'] || 0)
                    ) %>
              </span>
            </div>
          </div>
        </div>
      <% end %>
      
      <!-- Cost Estimate -->
      <% if analysis_request.metadata['total_cost'].present? %>
        <div>
          <h4 class="text-sm font-medium text-gray-700 mb-2">Cost Breakdown</h4>
          <div class="bg-gray-50 rounded-lg p-3">
            <div class="flex justify-between py-1 text-sm">
              <span class="text-gray-600 font-medium">Analysis Cost:</span>
              <span class="font-mono text-gray-900 font-medium">
                $<%= number_with_precision(analysis_request.metadata['total_cost'], precision: 4) %>
              </span>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </details>
</div>