                <div class="grid grid-cols-1 md:grid-cols-3 gap-4">
                  <div class="bg-gradient-to-br from-teal-50 to-cyan-50 rounded-lg p-4">
                    <div class="text-2xl font-bold text-teal-600">GPT-4</div>
                    <div class="text-sm text-gray-600">Intent Analysis</div>
                  </div>
                  <div class="bg-gradient-to-br from-cyan-50 to-sky-50 rounded-lg p-4">
                    <div class="text-2xl font-bold text-cyan-600">Python</div>
                    <div class="text-sm text-gray-600">Code Generated</div>
                  </div>
                  <div class="bg-gradient-to-br from-amber-50 to-orange-50 rounded-lg p-4">
                    <div class="text-2xl font-bold text-amber-600">92.3%</div>
                    <div class="text-sm text-gray-600">Predicted Retention</div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Advanced Features Grid -->
  <section class="py-20 px-4 sm:px-6 lg:px-8 bg-white">
    <div class="max-w-7xl mx-auto">
      <div class="text-center mb-12">
        <div class="inline-flex items-center gap-2 bg-teal-100 text-teal-800 px-4 py-2 rounded-full text-sm font-medium mb-4">
          <svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20">
            <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
