    <!-- Floating Elements Animation -->
    <div class="absolute inset-0 overflow-hidden">
      <div class="absolute -top-40 -right-40 w-80 h-80 bg-teal-300 rounded-full mix-blend-multiply filter blur-xl opacity-20 animate-blob"></div>
      <div class="absolute -bottom-40 -left-40 w-80 h-80 bg-amber-300 rounded-full mix-blend-multiply filter blur-xl opacity-20 animate-blob animation-delay-2000"></div>
      <div class="absolute top-40 left-1/2 w-80 h-80 bg-cyan-300 rounded-full mix-blend-multiply filter blur-xl opacity-20 animate-blob animation-delay-4000"></div>
    </div>

    <div class="relative max-w-7xl mx-auto">
      <div class="text-center">
        <!-- Trust Badge -->
        <div class="inline-flex items-center gap-2 bg-gradient-to-r from-teal-600 to-cyan-600 text-white px-4 py-2 rounded-full text-sm font-medium mb-6 animate-fade-in-down">
          <svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20">
            <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
          </svg>
          <span>Powered by GPT-4, Claude 3, Gemini & 7+ AI Models</span>
        </div>

        <!-- Main Headline with Gradient -->
        <h1 class="text-5xl md:text-7xl font-black mb-6 animate-fade-in-up">
          <span class="bg-clip-text text-transparent bg-gradient-to-r from-teal-600 via-cyan-600 to-sky-600">
            Ask Questions in English
          </span>
          <br>
          <span class="text-gray-900">Get Python, R & SQL Code</span>
        </h1>

        <!-- Value Proposition -->
        <p class="text-xl md:text-2xl text-gray-600 mb-8 max-w-3xl mx-auto animate-fade-in-up animation-delay-200">
          Connect PostgreSQL, MySQL, MongoDB, Snowflake, BigQuery, or upload CSV/Excel files. 
          Our multi-model AI orchestrator automatically selects the best model for your query.
        </p>

        <!-- Feature Pills -->
        <div class="flex flex-wrap justify-center gap-3 mb-10 animate-fade-in-up animation-delay-400">
          <span class="inline-flex items-center gap-2 px-4 py-2 bg-green-100 text-green-800 rounded-full text-sm font-medium">
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/>
            </svg>
            5-Stage Analysis Workflow
          </span>
          <span class="inline-flex items-center gap-2 px-4 py-2 bg-blue-100 text-blue-800 rounded-full text-sm font-medium">
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"/>
            </svg>
            Solid Cache Performance
          </span>
          <span class="inline-flex items-center gap-2 px-4 py-2 bg-amber-100 text-amber-800 rounded-full text-sm font-medium">
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"/>
            </svg>
            SOC2 & GDPR Compliant
          </span>
        </div>

        <!-- CTA Buttons -->
        <div class="flex flex-col sm:flex-row gap-4 justify-center animate-fade-in-up animation-delay-600">
          <% if user_signed_in? %>
            <%= link_to dashboards_path, class: "group relative inline-flex items-center justify-center px-8 py-4 text-lg font-bold text-white bg-gradient-to-r from-teal-600 to-cyan-600 rounded-xl hover:from-teal-700 hover:to-cyan-700 transform hover:scale-105 transition-all duration-200 shadow-xl hover:shadow-2xl" do %>
              <span class="relative z-10">Launch Dashboard</span>
              <svg class="w-5 h-5 ml-2 group-hover:translate-x-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7l5 5m0 0l-5 5m5-5H6"/>
              </svg>
              <div class="absolute inset-0 bg-gradient-to-r from-teal-600 to-cyan-600 rounded-xl blur opacity-40 group-hover:opacity-60 transition-opacity"></div>
            <% end %>
          <% else %>
            <%= link_to new_user_registration_path, class: "group relative inline-flex items-center justify-center px-8 py-4 text-lg font-bold text-white bg-gradient-to-r from-teal-600 to-cyan-600 rounded-xl hover:from-teal-700 hover:to-cyan-700 transform hover:scale-105 transition-all duration-200 shadow-xl hover:shadow-2xl" do %>
              <span class="relative z-10">Start Free Trial</span>
              <svg class="w-5 h-5 ml-2 group-hover:translate-x-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7l5 5m0 0l-5 5m5-5H6"/>
              </svg>
              <div class="absolute inset-0 bg-gradient-to-r from-teal-600 to-cyan-600 rounded-xl blur opacity-40 group-hover:opacity-60 transition-opacity"></div>
            <% end %>
            <%= link_to "Watch Demo", "#demo", class: "group inline-flex items-center justify-center px-8 py-4 text-lg font-bold text-gray-900 bg-white border-2 border-gray-200 rounded-xl hover:border-gray-300 hover:bg-gray-50 transform hover:scale-105 transition-all duration-200" %>
          <% end %>
        </div>

        <!-- Social Proof -->
        <div class="mt-12 flex flex-wrap items-center justify-center gap-8 text-gray-400">
          <div class="font-semibold text-lg">Trusted by teams at:</div>
          <span class="font-bold text-2xl">Fortune 500</span>
          <span class="font-bold text-2xl">Startups</span>
          <span class="font-bold text-2xl">Universities</span>
          <span class="font-bold text-2xl">Agencies</span>
        </div>
      </div>
    </div>
  </section>

  <!-- Interactive Demo Section -->
